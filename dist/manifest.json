{
  "manifest_version": 3,
  "name": "A11y Toolkit",
  "version": "1.0",
  "description": "A11y Toolkit Extension",
  "action": {
    "default_title": "A11y Toolkit",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "icons": {
    "16": "toolkit-ext-icon-16.png",
    "32": "toolkit-ext-icon-32.png",
    "48": "toolkit-ext-icon-48.png"
  }
}
